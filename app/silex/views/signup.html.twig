{% extends 'layout.html.twig' %}

{% block content %}
    <h1 class="cover-heading">Sign up</h1>
    <form method="post">
        Name: <input name="name" type="text" maxlength="255" /><br/>
        Email: <input name="email" type="text" maxlength="255" /><br/>
        Password: <input name="password" type="password" maxlength="255" /><br/>
        <input type="submit" value="Signup">
    </form>
{% endblock %}
